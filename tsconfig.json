{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "./",
    "jsxImportSource": "react",
    "paths": {
      "@*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "dist"
  ],
  "vueCompilerOptions": {
    "target": 3,
    "lib": "nativescript-vue"
  }
}